class MyrArray {
    initialize() {
        # println("BUILD NEW ARRAY")
        # self.each((e) => println(e))
    }

    push(e) { self[self.length()]=e }

    each(fn) { 
        i=0;
        max=self.length()
        while (i<max) {
            fn(self[i])
            i=i+1;
        }
    }
}


